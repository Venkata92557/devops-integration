pipeline{
    agent any
    tools{
        maven 'maven3.6.3'
    }
        stages{

            stage('Checkout SCM'){
                steps{
                  checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Venkata92557/devops-integration']]])

                }
            }
             stage('Build Maven Project'){
             steps{
             bat 'mvn clean install'
             }
             }
        }
    }